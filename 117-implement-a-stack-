public class Stack {
    List<Object> pilha = new List<Object>();
    
    public void push(Object obj) {
        pilha.add(0, obj);
    }
    
    public Object pop() {
        if (isEmpty()) {
            return null;
        }
        return pilha.remove(0);
    }
    
    public Integer size() {
        return pilha.size();
    }
    
    public Object peek(){
        Object resultado = pilha.get(0);
        return resultado;
    }
    
    public Boolean isEmpty() {
        Boolean resultado = pilha.isEmpty();

        return resultado;
    }
}
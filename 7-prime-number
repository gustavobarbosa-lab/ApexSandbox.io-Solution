public Boolean isPrime(Integer num) {
    Boolean resultado = true;

    for(Integer i = 2; i < num; i++) {
        if(i == num) {
            break;
        }
        else {
            if((Math.mod(num, i) == 0)) {
                resultado = false;
                break;
            }
        }
    }

   
    return resultado;
    
}
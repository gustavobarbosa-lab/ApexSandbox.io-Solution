public static List<Contact> ContactDeduplicator(List<Contact> contacts) {
    List<Contact> resultado = new List<Contact>();
    List<String> emails = new List<String>();

    if(contacts == null) {
        return new List<Contact>();
    }

    if(contacts.isEmpty()) {
        return new List<Contact>();
    }
    
   

    for(Contact contato : contacts) {
        if(contato.email != null) {
            if(!emails.contains(contato.email.toLowerCase())) {
                emails.add(contato.email.toLowerCase());
                resultado.add(contato);
            }
        }

        
    }

    return resultado;

}